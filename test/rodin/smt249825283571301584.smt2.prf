(declare-type Z ())
(declare-const input Bool)
(declare-const reg Bool)
(declare-const nf Z)
(declare-const flash Z)
(declare-const circuit Bool)
(define @t1 () (not input))
(define @t2 () (not reg))
(define @t3 () (and input @t2))
(assume @p1 circuit)
(assume @p2 (= nf flash))
(assume @p3 (not (or @t3 @t1 reg)))
(step @p4 (not @t3) :rule not_or_elim :premises (@p3) :args (0))
(step @p5 @t2 :rule not_or_elim :premises (@p3) :args (2))
(step @p6 (not @t1) :rule not_or_elim :premises (@p3) :args (1))
(step @p7 input :rule not_not_elim :premises (@p6))
(step @p8 @t3 :rule and_intro :premises (@p7 @p5))
(step @p9 false :rule contra :premises (@p8 @p4))
